public void recordIllegalComponentNameInRecord(RecordComponent recComp, TypeDeclaration typeDecl) {
	this.handle(
		IProblem.RecordIllegalComponentNameInRecord,
		new String[] {
				new String(recComp.name), new String(typeDecl.name)
			},
			new String[] {
					new String(recComp.name), new String(typeDecl.name)
			},
		recComp.sourceStart,
		recComp.sourceEnd);
}

