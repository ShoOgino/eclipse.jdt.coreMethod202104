public void test140() {
	Map settings = getCompilerOptions();
	settings.put(CompilerOptions.OPTION_PreserveUnusedLocal, CompilerOptions.OPTIMIZE_OUT);
	this.runConformTest(
		new String[] {
			"X.java",
			"class A {\n" + 
			"        long foo() {\n" + 
			"                return 0L;\n" + 
			"        }\n" + 
			"}\n" + 
			"\n" + 
			"public class X {\n" + 
			"        public static void main(String[] args) {\n" + 
			"                A a = new A();\n" + 
			"	             Long s = a.foo();\n" + 
			"                System.out.println(\"SUCCESS\");\n" + 
			"        }\n" + 
			"}\n", // =================
		},
		"SUCCESS",
		null,
		true,
		null,
		settings,
		null);		
}

