// null analysis - for
// https://bugs.eclipse.org/bugs/show_bug.cgi?id=176472
// variant
public void test0741_for_explicit_label() {
	this.runConformTest(
		new String[] {
			"X.java",
			"public class X {\n" +
			"  boolean test() {\n" +
			"    return true;\n" +
			"  }\n" + 
			"  void foo() {\n" + 
			"    Object o = null;\n" + 
			"    done: for (;;) {\n" + 
			"      if (test()) {\n" + 
			"        break done;\n" + 
			"      }\n" +
			"      o = new Object();\n" + 
			"    }\n" + 
			"    if (o == null) {\n" + 
			"    }\n" + 
			"  }\n" + 
			"}\n"},
		"");
} 

