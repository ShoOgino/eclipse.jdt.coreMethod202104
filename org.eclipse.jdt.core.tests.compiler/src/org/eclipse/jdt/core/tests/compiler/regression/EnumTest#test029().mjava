// check enum name visibility
public void test029() {
	this.runConformTest(
		new String[] {
			"X.java",
			"public class X {\n" + 
			"	enum Couleur { \n" + 
			"		BLEU, BLANC, ROUGE; // take precedence over toplevel BLEU type\n" + 
			"	}\n" + 
			"	class Y {\n" + 
			"		void foo(Couleur c) {\n" + 
			"			switch (c) {\n" + 
			"				case BLEU :\n" + 
			"					break;\n" + 
			"				case BLANC :\n" + 
			"					break;\n" + 
			"				case ROUGE :\n" + 
			"					break;\n" + 
			"			}\n" + 
			"		}	\n" + 
			"	}\n" + 
			"}\n" + 
			"\n" + 
			"class BLEU {}\n",
		},
		"");
}		

