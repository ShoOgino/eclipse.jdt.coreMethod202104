// null analysis -- try/finally
// https://bugs.eclipse.org/bugs/show_bug.cgi?id=177863
public void test0532_try_finally() {
	this.runConformTest(
		new String[] {
			"X.java",
			"public class X {\n" + 
			"  public void foo() {\n" + 
			"    Object o = null;\n" + 
			"    try {\n" + 
			"    } finally {\n" + 
			"      o = Object.class.getClass();\n" + 
			"      o.getClass();\n" + 
			"    }\n" + 
			"  }\n" + 
			"}"},
		"");
}

