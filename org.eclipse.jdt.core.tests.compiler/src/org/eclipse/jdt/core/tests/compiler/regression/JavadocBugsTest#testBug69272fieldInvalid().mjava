	public void testBug69272fieldInvalid() {
		runNegativeTest(
			new String[] {
				"X.java",
				"public class X {\n" + 
					"	int field;\n" + 
					"	/**@see #field* */\n" + 
					"	public void foo1() {}\n" + 
					"	/**@see #field*** ***/\n" + 
					"	public void foo2() {}\n" + 
					"	/**@see #field***\n" + 
					"	 */\n" + 
					"	public void foo3() {}\n" + 
					"}\n"
			},
			"----------\n" + 
				"1. ERROR in X.java (at line 3)\n" + 
				"	/**@see #field* */\n" + 
				"	        ^^^^^^^\n" + 
				"Javadoc: Malformed reference (missing end space separator)\n" + 
				"----------\n" + 
				"2. ERROR in X.java (at line 5)\n" + 
				"	/**@see #field*** ***/\n" + 
				"	        ^^^^^^^^^\n" + 
				"Javadoc: Malformed reference (missing end space separator)\n" + 
				"----------\n" + 
				"3. ERROR in X.java (at line 7)\n" + 
				"	/**@see #field***\n" + 
				"	        ^^^^^^^^^\n" + 
				"Javadoc: Malformed reference (missing end space separator)\n" + 
				"----------\n"
		);
	}

