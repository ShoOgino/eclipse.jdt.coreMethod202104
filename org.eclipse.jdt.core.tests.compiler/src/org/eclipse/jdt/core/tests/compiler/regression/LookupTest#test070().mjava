public void test070() {
	this.runConformTest(
			new String[] {
				"X.java",//===================
				"public class X {\n" + 
				"	interface I {\n" + 
				"	}\n" + 
				"\n" + 
				"	void test() {\n" + 
				"		new I() {\n" + 
				"			void foo() {\n" + 
				"			}\n" + 
				"		}.foo(); // compiles OK.\n" + 
				"		new I() {\n" + 
				"			void $foo() {\n" + 
				"			}\n" + 
				"		}.$foo(); // The method $foo() is undefined for the type new T.I(){}\n" + 
				"	}\n" + 
				"}", // =================
			},
			"");
}

