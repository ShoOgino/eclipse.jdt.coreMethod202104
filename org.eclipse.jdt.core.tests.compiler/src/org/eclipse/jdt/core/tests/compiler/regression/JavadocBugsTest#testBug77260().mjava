	/**
	 * Test bug 77260: [Javadoc] deprecation warning should not be reported when @deprecated tag is set
	 */
	public void testBug77260() {
		runConformTest(
			new String[] {
				"X.java",
				"/** @deprecated */\n" + 
					"public class X {\n" + 
					"	public int x;\n" + 
					"	public void foo() {}\n" + 
					"}\n",
				"Y.java",
				"/**\n" + 
					" * @see X\n" + 
					" * @deprecated\n" + 
					" */\n" + 
					"public class Y {\n" + 
					"	/** @see X#x */\n" + 
					"	public int x;\n" + 
					"	/** @see X#foo() */\n" + 
					"	public void foo() {}\n" + 
					"}\n",
				"Z.java",
				"public class Z {\n" + 
					"	/** \n" + 
					"	 * @see X#x\n" + 
					"	 * @deprecated\n" + 
					"	 */\n" + 
					"	public int x;\n" + 
					"	/**\n" + 
					"	 * @see X#foo()\n" + 
					"	 * @deprecated\n" + 
					"	 */\n" + 
					"	public void foo() {}\n" + 
					"}\n" }
		);
	}

