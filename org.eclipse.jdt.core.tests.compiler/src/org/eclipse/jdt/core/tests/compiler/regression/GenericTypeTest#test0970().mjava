				"        }\n" + 
				"}\n" + 
				"class XSub<E> implements X<E> {\n" + 
				"    I1<E> i1 = null;\n" + 
				"    I1.I2<E> i2 = null;\n" + 
				"    I1<E>.I2<E> i1i2 = null;\n" + 
