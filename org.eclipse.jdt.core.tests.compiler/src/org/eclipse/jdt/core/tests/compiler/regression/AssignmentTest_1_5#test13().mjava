public void test13() {
	this.runNegativeTest(
		new String[] {
			"X.java",
			"public class X {\n" +
			"	public static void main(String[] args) {\n" +
			"		Integer value = 4711;\n" +
			"		long test = 47L;\n" +
			"		value &= test;\n" +
			"	}\n" +
			"}",
		},
		"----------\n" +
		"1. ERROR in X.java (at line 5)\n" +
		"	value &= test;\n" +
		"	^^^^^^^^^^^^^\n" +
		"The operator &= is undefined for the argument type(s) Integer, long\n" +
		"----------\n");
}

