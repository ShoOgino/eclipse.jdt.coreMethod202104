// null analysis - do while
// https://bugs.eclipse.org/bugs/show_bug.cgi?id=147118
public void test0613_do_while() {
	this.runConformTest(
		new String[] {
			"X.java",
			"public class X {\n" + 
			"  String f;\n" + 
			"  void foo (boolean b) {\n" +
			"    X x = new X();\n" + 
			"    do {\n" + 
			"      System.out.println(x.f);\n" + 
			"      if (b) {\n" + 
			"        x = null;\n" + 
			"      }\n" + 
			"    } while (x != null);\n" + 
			"  }\n" + 
			"}\n"},
		"");
} 

