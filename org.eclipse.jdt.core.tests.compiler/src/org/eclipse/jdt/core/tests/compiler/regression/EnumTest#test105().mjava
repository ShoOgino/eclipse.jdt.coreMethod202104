	public void test105() {
		this.runConformTest(
				new String[] {
					"pack/X.java",
					"package pack;\n" +
					"import static pack.Color.*;\n" +
					"public class X {\n" +
					"    public static void main(String[] args) {\n" +
					"        Color c = BLACK;\n" +
					"        switch(c) {\n" +
					"        case BLACK:\n" +
					"            System.out.print(\"Black\");\n" +
					"            break;\n" +
					"        case WHITE:\n" +
					"            System.out.print(\"White\");\n" +
					"            break;\n" +
					"        }\n" +
					"    }\n" +
					"}",
					"pack/Color.java",
					"package pack;\n" + 
					"enum Color {WHITE, BLACK}"
				},
				"Black"
			);
		
		this.runConformTest(
			new String[] {
				"pack/Color.java",
				"package pack;\n" + 
				"enum Color {BLACK, WHITE}"
			},
			"",
			null,
			false,
			null
		);
		
		this.executeClass(
			"pack/X.java",
			"Black",
			null,
			false,
			null,
			null,
			null);	
	}

