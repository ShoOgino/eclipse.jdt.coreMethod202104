public void _test121() {
	this.runNegativeTest(
		new String[] {
			"X.java",
			"public enum X {\n" +
			"\n" +
			"	UNKNOWN();\n" +
			"\n" +
			"	private static String error;\n" +
			"\n" +
			"	{\n" +
			"		error = \"error\";\n" +
			"	}\n" +
			"\n" +
			"}\n",
		},
		"----------\n" +
		"1. ERROR in X.java (at line 8)\n" +
		"	error = \"error\";\n" +
		"	^^^^^\n" +
		"Cannot refer to the static enum field X.error within an initializer\n" +
		"----------\n");
}

