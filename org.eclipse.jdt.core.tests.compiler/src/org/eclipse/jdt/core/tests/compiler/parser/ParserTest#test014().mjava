public void test014() {
	this.runNegativeTest(
		new String[] {
			"X.java",
			"public class X {\n" +
			"	public void foo() { \n" +
			"		import java.lang.*;\n" +
			"	} \n" +
			"}\n"
		},
		"----------\n" + 
		"1. ERROR in X.java (at line 3)\n" + 
		"	import java.lang.*;\n" + 
		"	^^^^^^\n" + 
		"Syntax error on token \"import\", delete this token\n" + 
		"----------\n" + 
		"2. ERROR in X.java (at line 3)\n" + 
		"	import java.lang.*;\n" + 
		"	^^^^^^^^^^^^^^^^^\n" + 
		"Syntax error on token(s), misplaced construct(s)\n" + 
		"----------\n"
	);
}

