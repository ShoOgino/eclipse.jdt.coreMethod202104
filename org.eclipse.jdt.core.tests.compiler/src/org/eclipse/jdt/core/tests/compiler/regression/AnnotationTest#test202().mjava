public void test202() {
	this.runNegativeTest(
		new String[] {
			"X.java",
			" public class X {\n" + 
			" @Ann(m=Object)\n" + 
			" private int foo;\n" + 
			" private NonExisting bar;\n" + 
			" }\n" + 
			" @interface Ann {\n" + 
			" String m();\n" + 
			" }\n",
		},
		"----------\n" + 
		"1. ERROR in X.java (at line 2)\n" + 
		"	@Ann(m=Object)\n" + 
		"	       ^^^^^^\n" + 
		"Object cannot be resolved\n" + 
		"----------\n" + 
		"2. ERROR in X.java (at line 4)\n" + 
		"	private NonExisting bar;\n" + 
		"	        ^^^^^^^^^^^\n" + 
		"NonExisting cannot be resolved to a type\n" + 
		"----------\n");
}

