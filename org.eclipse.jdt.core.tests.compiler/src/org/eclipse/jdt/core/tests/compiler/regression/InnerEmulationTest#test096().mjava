public void test096() {
	this.runNegativeTest(
		new String[] {
			"X.java",
			"public class X {	\n"+
			"	static class Y {}	\n"+
			"	void foo() {	\n"+
			"		new X().new Y(){};	\n"+
			"	}	\n"+
			"}	\n",
		},
		"----------\n" + 
		"1. ERROR in X.java (at line 4)\n" + 
		"	new X().new Y(){};	\n" + 
		"	^^^^^^^\n" + 
		"Illegal enclosing instance specification for type X.Y\n" + 
		"----------\n");
}

