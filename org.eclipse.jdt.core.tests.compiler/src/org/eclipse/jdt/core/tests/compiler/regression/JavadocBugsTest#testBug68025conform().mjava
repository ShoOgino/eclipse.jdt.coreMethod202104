	/**
	 * Test fix for bug 68025: Javadoc processing does not detect some wrong links
	 * @see <a href="http://bugs.eclipse.org/bugs/show_bug.cgi?id=68025">68025</a>
	 */
	public void testBug68025conform() {
		runConformTest(
			new String[] {
				"Y.java",
				"public class Y {\n" + 
					"	public int field;\n" + 
					"	public void foo() {}\n" + 
					"}\n",
				"Z.java",
				"public class Z {\n" + 
					"	/**\n" + 
					"	 *	@see Y#field #valid\n" + 
					"	 *	@see Y#foo #valid\n" + 
					"	 */\n" + 
					"	public void foo1() {}\n" + 
					"	/**@see Y#field     # valid*/\n" + 
					"	public void foo2() {}\n" + 
					"	/**@see Y#foo		# valid*/\n" + 
					"	public void foo3() {}\n" + 
					"	/**@see Y#foo()\n" + 
					"	 *# valid*/\n" + 
					"	public void foo4() {}\n" + 
					"}\n"
			}
		);
	}

