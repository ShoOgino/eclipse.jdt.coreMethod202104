public void test054() {
	this.runConformTest(
		new String[] {
			"X.java",
			"public class X {\n" + 
			"  void bar(Z x) {\n" + 
			"    System.out.println(\"bar(Z)\");\n" + 
			"  }\n" + 
			"  void bar(I<?> x) {\n" + 
			"    System.out.println(\"bar(I)\");\n" + 
			"  }\n" + 
			"  public static void main(String args[]) {\n" + 
			"    (new X()).bar(new Z());\n" + 
			"  }\n" + 
			"}\n" + 
			"interface I<T> {}\n" + 
			"class Z implements I<Object> {}"
		},
		"bar(Z)");
}

