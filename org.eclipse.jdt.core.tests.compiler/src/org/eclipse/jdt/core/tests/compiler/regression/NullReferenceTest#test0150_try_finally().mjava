	// null analysis -- try/finally
	public void test0150_try_finally() {
		this.runNegativeTest(
			new String[] {
				"X.java",
				"public class X {\n" + 
				"	 Object m;\n" + 
				"	 void foo() {\n" + 
				"		 Object o = null;\n" + 
				"		 try {}\n" + 
				"		 finally {\n" + 
				"		   o = m;\n" + 
				"		 }\n" + 
				"		 o.toString();\n" + 
				"	 }\n" + 
				"}\n"},
			"" // because finally assigns to unknown value
		);
	}

