// null analysis -- strings concatenation
// https://bugs.eclipse.org/bugs/show_bug.cgi?id=132867
public void test0127_strings_concatenation() {
	this.runConformTest(
		new String[] {
			"X.java",
			"public class X {\n" + 
			"  void foo() {\n" +
			"    Object o = null;\n" + 
			"    System.out.println(o + \"\");\n" +
			"  }\n" + 
			"}\n"},
		"");
}

