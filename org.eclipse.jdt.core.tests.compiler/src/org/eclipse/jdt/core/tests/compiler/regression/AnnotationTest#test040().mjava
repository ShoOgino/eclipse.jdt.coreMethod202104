	// check annotation array field initializer
	public void test040() {
		this.runNegativeTest(
			new String[] {
				"X.java",
				"public @interface X {\n" + 
				"	int[] tab;\n" + 
				"	int[] value();\n" + 
				"}\n" 
			},
			"----------\n" + 
			"1. ERROR in X.java (at line 2)\n" + 
			"	int[] tab;\n" + 
			"	      ^^^\n" + 
			"The blank final field tab may not have been initialized\n" + 
			"----------\n");
	}		

