// https://bugs.eclipse.org/bugs/show_bug.cgi?id=162065
// variant - extending instead of implementing
// **
public void test037() {
	this.runConformTest(
		new String[] {
			"X.java",
			"interface I {\n" + 
			"  Object foo(Number n);\n" + 
			"}\n" + 
			"abstract class J {\n" + 
			"  abstract String foo(Number n);\n" + 
			"}\n" + 
			"public abstract class X extends J implements I {\n" + 
			"  void bar() {\n" + 
			"    foo(0.0f);\n" +
			"  }\n" + 
			"}"
		},
		"");
}

