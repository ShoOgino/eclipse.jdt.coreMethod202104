// check using an enum constant
public void test004() {
	this.runConformTest(
		new String[] {
			"X.java",
			"public enum X { \n" + 
			"	\n" + 
			"	BLEU,\n" + 
			"	BLANC,\n" + 
			"	ROUGE;\n" + 
			"	\n" + 
			"	public static void main(String[] args) {\n" + 
			"		System.out.println(BLEU);\n" + 
			"	}\n" + 
			"	\n" + 
			"}\n"
		},
		"BLEU");
}

