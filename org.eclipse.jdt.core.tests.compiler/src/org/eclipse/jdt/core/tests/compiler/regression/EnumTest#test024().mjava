// 77244 - cannot declare final enum
public void test024() {
	this.runNegativeTest(
		new String[] {
			"X.java",	
			"public final enum X {\n" +
			"	FOO() {}\n" +
			"}\n" + 
			"\n",
		},
	"----------\n" + 
	"1. ERROR in X.java (at line 1)\n" + 
	"	public final enum X {\n" + 
	"	                  ^\n" + 
	"Illegal modifier for the enum X; only public is permitted\n" + 
	"----------\n");
}	

