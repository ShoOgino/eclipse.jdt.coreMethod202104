	public void test064() throws ClassFormatException, IOException {
		String source =
			"public class X {\n" + 
			"	static boolean boom() { \n" + 
			"		throw new NullPointerException();\n" + 
			"	}\n" + 
			"	public static void main(String[] args) {\n" + 
			"		bar4();\n" + 
			"	}\n" + 
			"	static void bar4() {\n" + 
			"		int i = 6;\n" + 
			"		if ((true || boom()) || (i == 6)) {\n" + 
			"		   	System.out.println(i);\n" + 
			"		   }\n" + 
			"	}\n" + 
			"}";
		String expectedOutput =
			"  // Method descriptor #6 ()V\n" + 
			"  // Stack: 2, Locals: 1\n" + 
			"  static void bar4();\n" + 
			"     0  bipush 6\n" + 
			"     2  istore_0 [i]\n" + 
			"     3  getstatic java.lang.System.out : java.io.PrintStream [26]\n" + 
			"     6  iload_0 [i]\n" + 
			"     7  invokevirtual java.io.PrintStream.println(int) : void [32]\n" + 
			"    10  return\n" + 
			"      Line numbers:\n" + 
			"        [pc: 0, line: 9]\n" + 
			"        [pc: 3, line: 11]\n" + 
			"        [pc: 10, line: 13]\n" + 
			"      Local variable table:\n" + 
			"        [pc: 3, pc: 11] local: i index: 0 type: int\n";
		checkClassFile("X", source, expectedOutput);
	}

