//https://bugs.eclipse.org/bugs/show_bug.cgi?id=227502 - variation
public void _test150() throws Exception {
	this.runConformTest(
			new String[] {
					"p/X.java",
					"package p;\n" +
					"public class X {\n" +
					"	public enum E implements Runnable {\n" +
					"		SUCCESS;\n" +
					"		public void run(){}\n" +
					"	}\n" +
					"	public static void main(String[] args) {\n" +
					"		Class<E> c = E.class;\n" +
					"		System.out.println(c.getName() + \":\" + X.E.SUCCESS);\n" +
					"	}\n" +
					"}\n"
			},
			"p.X$E:SUCCESS");

	this.runConformTest(
			new String[] {
					"Y.java",
					"import p.X;\n" +
					"public class Y {\n" +
					"	public static void main(String[] args) {\n" +
					"		System.out.println(X.E.SUCCESS);\n" +
					"	}\n" +
					"}\n"
			},
			"SUCCESS",
			null,
			false,
			null);
}

