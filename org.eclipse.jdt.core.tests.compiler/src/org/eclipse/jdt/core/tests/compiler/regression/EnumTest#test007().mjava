// tolerate user definition for $VALUES
public void test007() {
	this.runConformTest(
		new String[] {
			"X.java",
			"public enum X { \n" + 
			"	\n" + 
			"	BLEU,\n" + 
			"	BLANC,\n" + 
			"	ROUGE;\n" + 
			"	\n" + 
			"   int $VALUES;\n" +
			"	public static void main(String[] args) {\n" + 
			"		for(X x: X.values()) {\n" + 
			"			System.out.print(x);\n" + 
			"		}\n" + 
			"	}\n" + 
			"	\n" + 
			"}\n"
		},
		"BLEUBLANCROUGE");
}	

