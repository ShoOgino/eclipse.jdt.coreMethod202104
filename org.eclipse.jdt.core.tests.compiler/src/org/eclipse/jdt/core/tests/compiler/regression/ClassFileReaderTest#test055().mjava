	public void test055() {
		String source =
			"public class X {\n" + 
			"	static boolean boom() { \n" + 
			"		throw new NullPointerException();\n" + 
			"	}\n" + 
			"	public static void main(String[] args) {\n" + 
			"		bar2();\n" + 
			"	}\n" + 
			"	static void bar2() {\n" + 
			"		int i = 6;\n" + 
			"		if ((i == 6) || (boom() || true)) {\n" + 
			"		} else {\n" + 
			"			System.out.println(i);\n" + 
			"		}\n" + 
			"	}\n" + 
			"}";
		String expectedOutput =
			"  // Method descriptor #6 ()V\n" + 
			"  // Stack: 2, Locals: 1\n" + 
			"  static void bar2();\n" + 
			"     0  bipush 6\n" + 
