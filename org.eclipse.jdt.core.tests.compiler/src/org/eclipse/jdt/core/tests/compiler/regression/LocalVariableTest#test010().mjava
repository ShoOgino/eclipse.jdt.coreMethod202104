public void test010() {
	Map options = getCompilerOptions();
	options.put(
		CompilerOptions.OPTION_DocCommentSupport,
		CompilerOptions.ENABLED);
	this.runConformTest(new String[] {
		"p/X.java",
		"package p;\n" + 
		"/**\n" + 
		" * @see Y\n" + 
		" */\n" + 
		"public class X {\n" + 
		"}",
		"p/Y.java",
		"package p;\n" + 
		"class Z {\n" + 
		"}",
	},
	"",
	null,
	true,
	null,
	options,
	null);
}

