// tolerate user definition for $VALUES
public void test007() {
	this.runConformTest(
		new String[] {
			"X.java",
			"class X { \n" +
			"	public static void main(String[] args) {\n" +
			"     enum Y { \n" +
			"		\n" +
			"		BLEU,\n" +
			"		BLANC,\n" +
			"		ROUGE;\n" +
			"		\n" +
			"      int $VALUES;\n" +
			"		public static void main(String[] args) {\n" +
			"				for(Y y: Y.values()) {\n" +
			"					System.out.print(y);\n" +
			"			}\n" +
			"		}\n" +
			"		\n" +
			"	  }\n" +
			"	  Y.main(args);\n" +
			"	}\n" +
			"}\n"
		},
		"BLEUBLANCROUGE");
}

