public void test134() {
    this.runNegativeTest(
        new String[] {
            "X.java",
			"public enum X {\n" + 
			"    INITIAL ,\n" + 
			"    OPENED {\n" + 
			"        {\n" + 
			"            System.out.printf(\"After the %s constructor\\n\",INITIAL);\n" + 
			"        }\n" + 
			"    }\n" + 
			"}",
        },
        "----------\n" + 
		"1. ERROR in X.java (at line 5)\n" + 
		"	System.out.printf(\"After the %s constructor\\n\",INITIAL);\n" + 
		"	                                               ^^^^^^^\n" + 
		"Cannot refer to the static enum field X.INITIAL within an initializer\n" + 
		"----------\n");
}

