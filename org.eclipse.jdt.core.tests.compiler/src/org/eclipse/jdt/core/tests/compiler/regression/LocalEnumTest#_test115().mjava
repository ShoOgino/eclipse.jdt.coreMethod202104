public void _test115() {
    this.runNegativeTest(
        new String[] {
            "X.java",
			"public enum X {\n" +
			"	VALUE;\n" +
			"\n" +
			"	static int ASD;\n" +
			"	final static int CST = 0;\n" +
			"	\n" +
			"	private X() {\n" +
			"		VALUE = null;\n" +
			"		ASD = 5;\n" +
			"		X.VALUE = null;\n" +
			"		X.ASD = 5;\n" +
			"		\n" +
			"		System.out.println(CST);\n" +
			"	}\n" +
			"}\n",
