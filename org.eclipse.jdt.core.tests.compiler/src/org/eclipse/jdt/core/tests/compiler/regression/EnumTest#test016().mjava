// check enum constants with wrong arguments
public void test016() {
	this.runConformTest(
		new String[] {
			"X.java",
			"public enum X {\n" + 
			"	\n" + 
			"	BLEU(10) {\n" + 
			"		String foo() { // inner\n" + 
			"			return super.foo() + this.val;\n" + 
			"		}\n" + 
			"	},\n" + 
			"	BLANC(20),\n" + 
			"	ROUGE(30);\n" + 
			"\n" + 
			"	int val;\n" + 
			"	X(int val) {\n" + 
			"		this.val = val;\n" + 
			"	}\n" + 
			"	String foo() {  // outer\n" + 
			"		return this.name();\n" + 
			"	}\n" + 
			"	public static void main(String[] args) {\n" + 
			"		for(X x: values()) {\n" + 
			"			System.out.print(x.foo());\n" + 
			"		}\n" + 
			"	}\n" + 
			"}\n"
		},
		"BLEU10BLANCROUGE");
}			

