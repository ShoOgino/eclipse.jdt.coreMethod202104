public void test08() {

	String s = 
		"public class X { 											\n"+
		"	Object o[] = { new String(\"SUCCESS\") ; };				\n"+
		"}															\n"; 	

	String expectedSyntaxErrorDiagnosis =
		"----------\n"+
		"1. ERROR in <test> (at line 2)\n"+
		"	Object o[] = { new String(\"SUCCESS\") ; };				\n"+
		"	                                     ^\n"+
		"Syntax error on token \";\", , expected\n"+
		"----------\n";

	String testName = "<test>";
	checkParse(
		s.toCharArray(),
		expectedSyntaxErrorDiagnosis,
		testName);
}

