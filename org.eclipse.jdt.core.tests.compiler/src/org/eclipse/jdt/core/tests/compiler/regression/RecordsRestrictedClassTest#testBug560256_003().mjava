public void testBug560256_003() {
	this.runNegativeTest(
		new String[] {
			"X.java",
			"class X {\n"+
			"  class Inner {\n"+
			"	  record Point(int x, int y){}\n"+
			"  }\n" +
			"}",
		},
		"----------\n" + 
		"1. ERROR in X.java (at line 3)\n" + 
		"	record Point(int x, int y){}\n" + 
		"	       ^^^^^\n" + 
		"Nested Record is (implicitly) static and hence enclosing type should be static\n" + 
		"----------\n");
}

