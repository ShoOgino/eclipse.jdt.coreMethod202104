	this.runNegativeTest(
		new String[] {
			"X.java",
			"public class X {\n" + 
			"    public static void main(String[] args)\n" + 
			"    {\n" + 
			"        Object obj = true ? true : 17.3;\n" + 
			"		 Zork z;\n" +
			"    }\n" + 
			"}\n",
