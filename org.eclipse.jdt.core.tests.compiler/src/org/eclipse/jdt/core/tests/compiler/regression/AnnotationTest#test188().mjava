    public void test188() {
    	this.runNegativeTest(
    		new String[] {
				"X.java",
				"@interface A1 {\n" + 
				"	int[] values();\n" + 
				"}\n" + 
				"@A1(values = new int[] { 1, 2 })\n" + 
				"public class X {\n" + 
				"	public static void main(String[] args) {\n" + 
				"	}\n" + 
				"}",
    		},
    		"----------\n" + 
    		"1. ERROR in X.java (at line 4)\n" + 
    		"	@A1(values = new int[] { 1, 2 })\n" + 
    		"	             ^^^^^^^^^^^^^^^^^^\n" + 
    		"The value for annotation attribute A1.values must be an array initializer\n" + 
    		"----------\n");
    }

