	// null analysis -- attribute
	public void test0006_attribute_with_method_call() {
		this.runNegativeTest(
			new String[] {
				"X.java",
				"public class X {\n" + 
				"	 Object o;\n" + 
				"	 void foo() {\n" + 
				"		 o = null;\n" + 
				"		 bar();\n" + 
				"		 o.toString();\n" + 
				"	 }\n" + 
				"	 static void bar() {\n" + 
				"	 }\n" + 
				"}\n"},
			"" // still ok because the class may hold a pointer to this
		);
	}

