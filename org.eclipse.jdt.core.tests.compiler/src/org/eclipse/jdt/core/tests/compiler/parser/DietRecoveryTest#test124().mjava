			"    m2();\n" + 
			"    {\n" + 
			"      int c;\n" + 
			"      int d;\n" + 
			"    }\n" + 
			"  }\n" + 
			"}\n";
	} else {
		expectedDietPlusBodyPlusStatementsRecoveryUnitToString =
			"public class Test {\n" + 
			"  public Test() {\n" + 
			"    super();\n" + 
			"  }\n" + 
			"  void aMethod() {\n" + 
			"    public static @m1() enum $missing$ {\n" + 
			"      public $missing$() {\n" + 
			"        super();\n" + 
			"      }\n" + 
			"      <clinit>() {\n" + 
