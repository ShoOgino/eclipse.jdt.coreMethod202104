// check enum name visibility
public void test028() {
	this.runNegativeTest(
		new String[] {
			"X.java",
			"public class X {\n" + 
			"	enum Couleur { \n" + 
			"		BLEU, BLANC, ROUGE;\n" + 
			"		static int C = 0;\n" + 
			"		static void FOO() {}\n" + 
			"	}\n" + 
			"	class Y {\n" + 
			"		void foo(Couleur c) {\n" + 
			"			switch (c) {\n" + 
			"				case BLEU :\n" + 
			"					break;\n" + 
			"				case BLANC :\n" + 
			"					break;\n" + 
			"				case ROUGE :\n" + 
			"					break;\n" + 
			"			}\n" + 
			"			FOO();\n" + 
			"			C++;\n" + 
			"		}	\n" + 
			"	}\n" + 
			"}\n",
		},
		"----------\n" + 
		"1. ERROR in X.java (at line 17)\n" + 
		"	FOO();\n" + 
		"	^^^\n" + 
		"The method FOO() is undefined for the type X.Y\n" + 
		"----------\n" + 
		"2. ERROR in X.java (at line 18)\n" + 
		"	C++;\n" + 
		"	^\n" + 
		"C cannot be resolved\n" + 
		"----------\n");
}		

