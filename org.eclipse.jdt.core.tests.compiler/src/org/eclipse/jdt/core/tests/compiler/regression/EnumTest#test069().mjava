public void test069() {
    this.runNegativeTest(
        new String[] {
            "X.java",
            "public enum X {\n" +
            "    A(1, 3), B(1, 3), C(1, 3) { }\n" +
            "   	;\n" +
            "    protected X(int i, int j) { }\n" +
            "}",
        },
        "----------\n" + 
		"1. ERROR in X.java (at line 4)\n" + 
		"	protected X(int i, int j) { }\n" + 
		"	          ^^^^^^^^^^^^^^^\n" + 
		"Illegal modifier for the enum constructor; only private is permitted.\n" + 
		"----------\n");
}

