/*
 * Ensures that the source of an inner class can be retrieved.
 */
public void testInnerClass6() throws JavaModelException {
	IType type = this.innerClasses.getClassFile("X$1$W.class").getType();
	assertSourceEquals(
		"Unexpected source",
		"class W {\n" + 
		"      void bar() {\n" + 
		"        new W() {};\n" + 
		"      }\n" + 
		"    }",
		type.getSource());
}

