public void testAddPackageSourceIsBin() throws CoreException {
	try {
		createJavaProject("P", new String[] {"src"}, "src");
		startDeltas();
		createFolder("P/src/x");
		assertDeltas(
			"Unexpected delta", 
			"P[*]: {CHILDREN}\n" +
			"	src[*]: {CHILDREN}\n" +
			"		x[+]: {}"
		);
	} finally {
		stopDeltas();
		deleteProject("P");
	}
}

